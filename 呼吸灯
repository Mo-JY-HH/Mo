void fadeOff(int time) {
  for (int i = 255; i >= 0; i = i + (-5)) {
    analogWrite(10,i);
    delay((time / (255 / 5)));
  }
}

void fadeOn(int time) {
  for (int i = 0; i <= 255; i = i + (5)) {
    analogWrite(10,i);
    delay((time / (255 / 5)));
  }
}

void setup(){

}

void loop(){
  fadeOn(2000);
  fadeOff(2000);

}

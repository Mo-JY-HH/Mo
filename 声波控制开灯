volatile int noise;

void setup(){
  noise = 0;
  pinMode(6, OUTPUT);
  Serial.begin(9600);
}

void loop(){
  while (true) {
    noise = analogRead(A3);
    if (noise > 66) {
      digitalWrite(6,HIGH);
      Serial.println(noise);
      delay(5000);

    }
    if (noise <= 66) {
      Serial.println(noise);
      digitalWrite(6,LOW);

    }
  }

}
